{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "suppressImplicitAnyIndexErrors":true
  },
  "exclude": [
    "node_modules"
  ],
  "include": ["src/**/*"]
}
